
# gitlab服务器的host
gitlab_host_name=www.gitlab.cn
jenkins_host_name=www.jenkins.cn
gitlab_runner_host_name=www.gitlab-runner.com

# 服务器对外暴露的借口
gitlab_port=5000
jenkins_port=5002
gitlab_ssh_port=5004

# 挂载的资源文件存放在容器中的路径
external_common_mounts_path=/var/external-common-mounts

# jenkins初始化的账号
jenkins_user=root
jenkins_pass=12345678

# jenkins服务器配置
jenkins_home_path=/var/jenkins_home
jenkins_executer_folder_name=spb-builder
jenkins_add_credential_filename=add-credential-with-ssh-key.sh
